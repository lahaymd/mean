extends layout
block content 
	include header
	div(class='container' ng-controller="SidenavController as $ctrl" layout='row' flex)
		md-sidenav(flex class='md-sidenav-left md-whiteframe-z2' md-component-id='left'  ng-click='$ctrl.toggleNav()') 
			md-button( name='users' md-nav-click="goto('users')" ui-sref='users' ui-sref-active='active') Users
			md-divider
			//- md-button( name='register' md-nav-click="goto('register')" ui-sref='register' ui-sref-active='active') Register
			//- md-divider
			md-button( name='login' md-nav-click="goto('login')" ui-sref='login' ui-sref-active='active') Home
			md-divider			
			md-button( name='readerboard' md-nav-click="goto('readerboard')" ui-sref='readerboard' ui-sref-active='active') Readerboard
			md-divider
			md-button( name='pdf' md-nav-click="goto('pdf')" ui-sref='pdf' ui-sref-active='active') Resume
			md-divider
			md-button( name='hireme' md-nav-click="goto('hireme')" ui-sref='hireme' ui-sref-active='active') Hire Me
		div(ui-view flex)
		
	include footer
         



