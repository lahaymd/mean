div(ng-controller='NavController as vm')
		//- a#logo(href='/')
		//- 	include coat-of-arms.svg
		div session name {{vm.session}}
		div(ng-show='vm.isLogged()') you are logged in
		md-button(ng-click='vm.isLogged()') is logged?
		md-button(ng-click='vm.logout()') Log Out
		
		md-nav-bar( hide-xs   md-selected-nav-item='vm.currentNavItem'  layout='row' layout-align='space-around' layout-fill layout-margin layout-padding) 
			md-nav-item(  name='login' md-nav-click="goto('login')" ui-sref='login' ui-sref-active='active') Home
			//- md-nav-item( name='users' md-nav-click="goto('users')" ui-sref='users' ui-sref-active='active') Table Example
			//- md-nav-item( name='readerboard' md-nav-click="goto('readerboard')" ui-sref='readerboard' ui-sref-active='active') Letter Example
			md-nav-item( name='hireme' md-nav-click="goto('hireme')" ui-sref='hireme' ui-sref-active='active') Contact Me
			md-nav-item( name='pdf' md-nav-click="goto('pdf')" ui-sref='pdf' ui-sref-active='active') Resume
			md-menu(md-offset='0 50')
				md-button(ng-click='$mdOpenMenu($event)') Examples
				md-menu-content(ng-click='$mdCloseMenu($event)' width='4')
					md-menu-item
						md-button(ui-sref='users') Dynamic Users Table
					md-menu-item
						md-button(ui-sref='readerboard') Letters App
	
		md-button#hamburger( ng-click='vm.open()' hide show-xs )
			md-icon(md-svg-src='/static/material-design-icons/navigation/svg/production/ic_menu_24px.svg')
		form.signup( name='myForm1' ng-model="myForm1" ng-submit='vm.saveToMongoLab()') 
				//- div( ng-class="{ 'has-error': myForm.username.$touched && myForm.username.$invalid}")
				md-input-container
					label(for="fuck") Username<br />
					input#username(name="fuck" ng-model='vm.userlist.fuck' minlength='5' required) 
				md-input-container
					label(for="shit") password<br />
					input#password(name="shit" ng-model='vm.userlist.shit' minlength='5' required) 
				
				md-button.md-raised.md-primary(type="submit", value="Create") Create
		form.login( name='myForm2' ng-model="myForm2" ng-submit='vm.loginToMongoLab()') 
				//- div( ng-class="{ 'has-error': myForm.username.$touched && myForm.username.$invalid}")
				md-input-container
					label(for="fuck") Username<br />
					input#username(name="fuck" ng-model='vm.loginForm.fuck' minlength='5' required) 
				md-input-container
					label(for="shit") password<br />
					input#password(name="shit" ng-model='vm.loginForm.shit' minlength='5' required) 
				
				md-button.md-raised.md-primary(type="submit", value="Create") Create
		{{vm.userlist}}
